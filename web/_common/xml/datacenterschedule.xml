<datacenter>
	<exporters>
		<!-- 	interval format:
				simple interval: put interval time in milliseconds
				daily: DAILY:hhmm;hhmm;hhmm (executes every day at times indicated by hhmm)
				monthly: MONTHLY:d;hhmm,d;hhmm,d;hhmm (executes every month on days d at time indicated by hhmm)
				yearly: YEARLY:M;d,M;d,M;d (executes on every month M on day d at 00h00)
		-->
		<module class="be.openclinic.datacenter.ExporterCore" param="core.1" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.1.1" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.1.2" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.1.3" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.2" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.3" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.4" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.4.1" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.4.2" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.5" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.6" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.7" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.8" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.8.1" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.8.2" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.9" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.9.1" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.9.2" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.10" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.11" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.12" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.13" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.14" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.15" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.16" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.17" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterCore" param="core.18" interval="DAILY:0000"/>

		<module class="be.openclinic.datacenter.ExporterSystem" param="system.1" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterSystem" param="system.2" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterSystem" param="system.3" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterSystem" param="system.4" interval="DAILY:0000"/>
		<module class="be.openclinic.datacenter.ExporterSystem" param="system.5" interval="DAILY:0000"/>

		<module class="be.openclinic.datacenter.ExporterMedical" param="medical.1" interval="MONTHLY:15;0000"/>
	</exporters>
	<senders>
		<module class="be.openclinic.datacenter.SMTPSender" interval="300000"/>
	</senders>
	<receivers>
		<module class="be.openclinic.datacenter.POP3Receiver"/>
	</receivers>
	<parameters>
		<parameter param="core.1" 	type="simplevalue"/>
		<parameter param="core.1.1" type="simplevalue"/>
		<parameter param="core.1.2" type="simplevalue"/>
		<parameter param="core.1.3" type="simplevalue"/>
